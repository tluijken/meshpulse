{
    "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.10.7/.schema/devbox.schema.json",
    "packages": [
        "rustup@latest",
        "libiconv@latest"
    ],
    "shell": {
        "init_hook": [
            "rustup default stable"
        ],
        "scripts": {
            "test": "cargo test -- --show-output",
            "start" : "cargo run",
            "build-docs": "cargo doc",
            "clean": "cargo clean"
        }
    }
}

